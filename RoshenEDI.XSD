                            <xs:documentation xml:lang="ru">
                                Код страны alpha-2 согласно ISO 3166-1.
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                Country code (alpha-2 according ISO 3166-1)
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="SkuNationalId">
        <xs:sequence>
            <xs:element name="Id" type="tns:InstanceIdentifier">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Identifier
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CountryCode" type="tns:CountryCode">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Код страны alpha-2 согласно ISO 3166-1.
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Country code (alpha-2 according ISO 3166-1)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TradeItem">
        <xs:sequence>
            <xs:element name="TradeItemIdentifier" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор упаковки SKU - штрихкод. Может быть не заполнен только для таких упаковок, как ряд и паллета, либо при выгрузке SKU на этапе планирования.
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Barcode. It can be omitted only for such packages as a layer and a pallet, or for SKU at the planning stage.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PackageTypeCode">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Код типа упаковки (штука, блок, ящик, ряд, паллета)
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Package type code (KG,PCS,UNIT,BOX,LAYER,PALLET)
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="KG"/>
                        <xs:enumeration value="PCS"/>
                        <xs:enumeration value="UNIT"/>
                        <xs:enumeration value="BOX"/>
                        <xs:enumeration value="LAYER"/>
                        <xs:enumeration value="PALLET"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="QuantityOfSkuBaseUnits" type="tns:Quantity">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Количество базовых единиц SKU в данной упаковке.
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Quantity of BASE units (KG/PCS) in current package
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TradeItemWeightAndDimension" type="tns:TradeItemWeightAndDimension">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Вес и размеры упаковки
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Weight and dimensions of package
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TradeItemWeightAndDimension">
        <xs:sequence>
            <xs:element name="GrossWeight" type="tns:Weight" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Вес брутто в килограммах
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Gross weight in kilograms
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="NetWeight" type="tns:Weight">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Вес нетто в килограммах (тот вес который указан на упаковке)
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Net weight in kilograms (as indicated on the package)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Width" minOccurs="0" type="tns:Dimension">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Ширина упаковки в сантиметрах
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Width of the package in centimeters
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Height" minOccurs="0" type="tns:Dimension">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Высота упаковки в сантиметрах
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Height of the package in centimeters
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Depth" minOccurs="0" type="tns:Dimension">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Глубина упаковки в сантиметрах
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Depth of the package in centimeters
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TradeItems">
        <xs:sequence>
            <xs:element name="TradeItem" type="tns:TradeItem" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Упаковка
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Package
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="OwnGoodsInStockList">
    <xs:sequence>
    <xs:element name="OwnGoodsInStock" type="tns:OwnGoodsInStock" maxOccurs="unbounded">
        <xs:annotation>
            <xs:documentation xml:lang="ru">
                Остатки товаров
            </xs:documentation>
            <xs:documentation xml:lang="en">
                Goods in stock
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    </xs:sequence>
    </xs:complexType>


    <xs:complexType name="OwnGoodsInStock">
        <xs:sequence>
            <xs:element name="CompanyGLN" type="tns:GLN">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор компании
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Company identifier
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CompanyName" type="tns:Notes">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Название компании
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Company name
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="WarehouseGLN" type="tns:GLN">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор склада
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Warehouse identifier
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="WarehouseName" type="tns:Notes">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Название склада
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Warehouse name
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Date" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дата остатков
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Date of stock
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SKUList" type="tns:OwnGoodsInStockSKUList">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Список SKU
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        SKU list
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="OwnGoodsInStockSKUList">
        <xs:sequence>
            <xs:element name="SKU" type="tns:OwnGoodsInStockSKUItem" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        SKU
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        SKU
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="OwnGoodsInStockSKUItem">
        <xs:sequence>
            <xs:element name="SkuSourceIdentifier" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор SKU согласно настройки КУБ (Маркетинговый код, штрихкод ящика, код источника)
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        SKU identifier according to CUB setting (Marketing code, barcode of the box, source code)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Quantity" type="tns:Quantity">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Количество товара в БАЗОВОЙ единице измерения (КГ/ШТ)
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Quantity of BASE units (KG/PCS)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:simpleType name="Notes">
        <xs:restriction base="xs:string">
            <xs:maxLength value="200"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="CountryCode">
        <xs:restriction base="xs:string">
            <xs:minLength value="2"/>
            <xs:maxLength value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="LanguageCode">
        <xs:restriction base="xs:string">
            <xs:minLength value="2"/>
            <xs:maxLength value="2"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="CurrencyCode">
        <xs:restriction base="xs:string">
            <xs:minLength value="3"/>
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="GLN">
        <xs:restriction base="xs:string">
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="DocumentNumber">
        <xs:restriction base="xs:string">
            <xs:maxLength value="35"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="InstanceIdentifier">
        <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>

     <xs:simpleType name = "Expiration">
         <xs:restriction base="xs:int">
             <xs:totalDigits value="4"/>
         </xs:restriction>
     </xs:simpleType>

    <xs:simpleType name="LineNumber">
        <xs:restriction base="xs:decimal">
            <xs:fractionDigits value="0"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="Quantity">
        <xs:restriction base="xs:decimal">
            <xs:fractionDigits value="3"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="Amount">
        <xs:restriction base="xs:decimal">
            <xs:fractionDigits value="2"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="Dimension">
        <xs:restriction base="xs:decimal">
            <xs:fractionDigits value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="Weight">
        <xs:restriction base="xs:decimal">
            <xs:fractionDigits value="6"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DocumentStatus">
        <xs:restriction base="xs:string">
            <xs:enumeration value="POSTED"/>
            <xs:enumeration value="CANCELED"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="GoodsDocumentType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="SALE"/>
            <xs:enumeration value="RETURN"/>
            <xs:enumeration value="CORRECTION"/>
        </xs:restriction>
    </xs:simpleType>

</xs:schema>