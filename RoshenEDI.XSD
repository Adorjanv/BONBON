<xs:schema xmlns:tns="http://www.roshen.ua/edi/roshen4" xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.roshen.ua/edi/roshen4" attributeFormDefault="unqualified"
           elementFormDefault="qualified">
    <xs:element name="ExchangeMessage" type="tns:ExchangeMessage">
        <xs:annotation>
            <xs:documentation xml:lang="ru">
                Корневой элемент документа
            </xs:documentation>
            <xs:documentation xml:lang="en">
                Document root node
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:complexType name="ExchangeMessage">
        <xs:sequence>
            <xs:element name="Header" type="tns:MessageHeader">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Заголовок сообщения
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Message header
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="GoodsSales" type="tns:GoodsSales" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Список продаж
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        List of sales
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="GoodsReturns" type="tns:GoodsReturns" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Список возвратов
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        List of returns
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="GoodsSaleCorrections" type="tns:GoodsSaleCorrections" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Список корректировок
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        List of sale corrections
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SkuList" type="tns:SkuList" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Список SKU
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        List of SKU
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OwnGoodsInStock" type="tns:OwnGoodsInStockList" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Список остатков (на собственных складах)
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        List of goods in stock (own warehouses)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="MessageHeader">
        <xs:sequence>
            <xs:element name="ProtocolVersion" type="xs:string" fixed="1.20">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Версия протокола обмена
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Protocol version
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Sender" type="tns:InstanceIdentifier">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор БД отправителя согласно классификации CUB
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Sender DB identifier according to CUB classification
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Receiver" type="tns:InstanceIdentifier">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор БД получателя согласно классификации CUB
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Recipient DB identifier according to CUB classification
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="MessageId" type="tns:InstanceIdentifier">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор сообщения
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Message identifier
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ReplyToMessageId" type="tns:InstanceIdentifier">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор последнего полученного сообщения
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Identifier of last received message
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="GoodsSales">
        <xs:sequence>
            <xs:element name="Sale" type="tns:GoodsSale" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="GoodsReturns">
        <xs:sequence>
            <xs:element name="Return" type="tns:GoodsReturn" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="GoodsSaleCorrections">
        <xs:sequence>
            <xs:element name="SaleCorrection" type="tns:GoodsSaleCorrection" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="GoodsSale">
        <xs:sequence>
            <xs:element name="DocumentDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дата документа
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Document date
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DocumentNumber" type="tns:DocumentNumber">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Номер документа
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Document number
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DocumentStatusCode" type="tns:DocumentStatus">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Если документ отражен в учете (сформированы проводки) то выгружать со статусом Posted, во всех остальных случаях Canceled.
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        If the document is reflected in the account (formed wiring) then unload with the status Posted, in all other cases Canceled
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InstanceIdentifier" type="tns:InstanceIdentifier">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Уникальный идентификатор продажи (не должен дублироваться для разных документов, в т.ч. разных типов, разных лет)
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Sale unique identifier (It should not be duplicated for different documents, including different types, different years)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InvoiceDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дата инвойса
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Invoice date
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InvoiceNumber" type="tns:DocumentNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Номер инвойса
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Invoice number
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TransferOfOwnershipDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дата перехода права собственности
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Date of transfer of ownership
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ExportDeclarationDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дата экспортной декларации
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Export declaration date
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DocumentCurrencyCode" type="tns:CurrencyCode">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Код валюты согласно ISO 4217
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Currency code (according to ISO 4217)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Contract" type="tns:Contract">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Договор на поставку товаров
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Contract
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SellerInfo" type="tns:SellerInfo">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Информация о продавце
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Seller info
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BuyerInfo" type="tns:BuyerInfo">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Информация о покупателе
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Buyer info
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ShipFromInfo" type="tns:ShipFromInfo">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Информация об отправителе
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Sender info
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ShipToInfo" type="tns:ShipToInfo">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Информация о получателе
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Recipient info
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Notes" type="tns:Notes" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Произвольный комментарий к документу
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Comment
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SKUList" type="tns:SaleSKUList">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Список SKU продажи
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        List of SKU
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Contract">
        <xs:sequence>
            <xs:element name="ContractDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дата договора на поставку товаров
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Contract date
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ContractNumber" type="tns:DocumentNumber">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Номер договора на поставку товаров
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Contract number
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SellerInfo">
        <xs:sequence>
            <xs:element name="SellerCountryCode" type="tns:CountryCode">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Код страны продавца alpha-2 согласно ISO 3166-1
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Seller country code (alpha-2 according ISO 3166-1)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SellerGLN" type="tns:GLN">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор продавца
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Seller identifier
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SellerName" type="tns:Notes">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Название продавца
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Seller name
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SellerWarehouseGLN" type="tns:GLN">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор склада продавца
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Seller warehouse identifier
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SellerWarehouseName" type="tns:Notes">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Название склада продавца
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Seller warehouse name
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="BuyerInfo">
        <xs:sequence>
            <xs:element name="BuyerCountryCode" type="tns:CountryCode">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Код страны покупателя alpha-2 согласно ISO 3166-1
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Buyer country code (alpha-2 according ISO 3166-1)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BuyerGLN" type="tns:GLN">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор покупателя
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Buyer identifier
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BuyerName" type="tns:Notes">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Название покупателя
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Buyer name
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BuyerWarehouseGLN" type="tns:GLN">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор склада покупателя
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Buyer warehouse identifier
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BuyerWarehouseName" type="tns:Notes">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Название склада покупателя
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Buyer warehouse name
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ShipFromInfo">
        <xs:sequence>
            <xs:element name="ShippingDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дата отгрузки товаров со склада
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Shipping date
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ShipToInfo">
        <xs:sequence>
            <xs:element name="DeliveryDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дата приема груза получателем
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Delivery date
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SaleSKUList">
        <xs:sequence>
            <xs:element name="SaleSkuItem" type="tns:SaleSkuItem" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Sku продажи
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Sale SKU
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SaleSkuItem">
        <xs:sequence>
            <xs:element name="SkuSourceIdentifier" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор SKU согласно настройки КУБ (Маркетинговый код, штрихкод ящика, код источника)
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        SKU identifier according to CUB setting (Marketing code, barcode of the box, source code)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Quantity" type="tns:Quantity">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Количество товара в БАЗОВОЙ единице измерения (КГ/ШТ)
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Quantity of BASE units (KG/PCS)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TotalSkuAmountBeforeTaxes" type="tns:Amount">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Итоговая стоимость товара без учета налогов, без транспортных и маркетинговых затрат и без прочих дополнительных расходов
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Total value of the goods without taxes, without transport and marketing costs and without other additional costs
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="GoodsReturn">
        <xs:sequence>
            <xs:element name="DocumentDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дата документа
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Document date
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DocumentNumber" type="tns:DocumentNumber">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Номер документа
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Document number
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DocumentStatusCode" type="tns:DocumentStatus">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Если документ отражен в учете (сформированы проводки) то выгружать со статусом Posted, во всех остальных случаях Canceled.
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        If the document is reflected in the account (formed wiring) then unload with the status Posted, in all other cases Canceled
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InstanceIdentifier" type="tns:InstanceIdentifier">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Уникальный идентификатор возврата (не должен дублироваться для разных документов, в т.ч. разных типов, разных лет)
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Return unique identifier (It should not be duplicated for different documents, including different types, different years)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SaleIdentifier" type="tns:InstanceIdentifier" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Уникальный идентификатор продажи на основе которой делается возврат (не должен дублироваться для разных документов, в т.ч. разных типов, разных лет)
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Base sale document unique identifier (must not be duplicated for different documents, including various types of different years)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InvoiceDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дата инвойса
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Invoice date
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InvoiceNumber" type="tns:DocumentNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Номер инвойса
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Invoice number
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TransferOfOwnershipDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дата перехода права собственности
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Date of transfer of ownership
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ExportDeclarationDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дата экспортной декларации
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Export declaration date
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DocumentCurrencyCode" type="tns:CurrencyCode">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Код валюты согласно ISO 4217
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Currency code (according to ISO 4217)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Contract" type="tns:Contract">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Договор на поставку товаров
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Contract
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SellerInfo" type="tns:SellerInfo">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Информация о продавце
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Seller info
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BuyerInfo" type="tns:BuyerInfo">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Информация о покупателе
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Buyer info
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ShipFromInfo" type="tns:ShipFromInfo">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Информация об отправители
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Sender info
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ShipToInfo" type="tns:ShipToInfo">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Информация о получателе
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Recipient info
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Notes" type="tns:Notes" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Произвольный комментарий к документу
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Comment
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SKUList" type="tns:ReturnSKUList">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
