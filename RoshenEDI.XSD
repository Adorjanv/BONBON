    <xs:complexType name="GoodsSaleCorrection">
        <xs:sequence>
            <xs:element name="DocumentDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дата документа
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Document date
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DocumentNumber" type="tns:DocumentNumber">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Номер документа
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Document number
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DocumentStatusCode" type="tns:DocumentStatus">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Если документ отражен в учете (сформированы проводки) то выгружать со статусом Posted, во всех остальных случаях Canceled.
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        If the document is reflected in the account (formed wiring) then unload with the status Posted, in all other cases Canceled
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InstanceIdentifier" type="tns:InstanceIdentifier">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Уникальный идентификатор продажи (не должен дублироваться для разных документов, в т.ч. разных типов, разных лет)
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Base return document unique identifier (It should not be duplicated for different documents, including different types, different years)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OriginalInstanceType" type="tns:GoodsDocumentType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Тип основания
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Base document type
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OriginalInstanceIdentifier" type="tns:InstanceIdentifier" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Уникальный идентификатор продажи/возврата на основе которой делается корректировка (не должен дублироваться для разных документов, в т.ч. разных типов, разных лет)
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Original sale unique identifier (must not be duplicated for different documents, including various types of different years)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InvoiceDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дата инвойса
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Invoice date
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InvoiceNumber" type="tns:DocumentNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Номер инвойса
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Invoice number
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TransferOfOwnershipDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дата перехода права собственности
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Date of transfer of ownership
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DocumentCurrencyCode" type="tns:CurrencyCode">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Код валюты согласно ISO 4217
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Currency code (according to ISO 4217)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Contract" type="tns:Contract">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Договор на поставку товаров
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Contract
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SellerInfo" type="tns:SellerInfo">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Информация о продавце
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Seller info
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BuyerInfo" type="tns:BuyerInfo">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Информация о покупателе
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Buyer info
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ShipFromInfo" type="tns:ShipFromInfo">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Информация об отправители
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Sender info
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ShipToInfo" type="tns:ShipToInfo">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Информация о получателе
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Recipient info
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Notes" type="tns:Notes" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Произвольный комментарий к документу
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Comment
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SKUList" type="tns:SaleCorrectionSKUList">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Список SKU корректировки
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        List of SKU
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SaleCorrectionSKUList">
        <xs:sequence>
            <xs:element name="SkuItem" type="tns:SaleCorrectionSkuItem" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        SKU корректировки
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Correction SKU
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SaleCorrectionSkuItem">
        <xs:sequence>
            <xs:element name="SkuSourceIdentifier" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор SKU согласно настройки КУБ (Маркетинговый код, штрихкод ящика, код источника)
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        SKU identifier according to CUB setting (Marketing code, barcode of the box, source code)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="QuantityBeforeCorrection" type="tns:Quantity">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Количество товара в БАЗОВОЙ единице измерения ДО корректировки
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Quantity of BASE units (KG/PCS) BEFORE correction
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TotalSkuAmountBeforeTaxesBeforeCorrection" type="tns:Amount">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Итоговая стоимость товара без учета налогов, без транспортных и маркетинговых затрат и без прочих дополнительных расходов ДО корректировки
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Total value of the goods without taxes, without transport and marketing costs and without other additional costs BEFORE correction
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="QuantityAfterCorrection" type="tns:Quantity">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Количество товара в БАЗОВОЙ единице измерения ПОСЛЕ корректировки
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Quantity of BASE units (KG/PCS) AFTER correction
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TotalSkuAmountBeforeTaxesAfterCorrection" type="tns:Amount">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Итоговая стоимость товара без учета налогов, без транспортных и маркетинговых затрат и без прочих дополнительных расходов ПОСЛЕ корректировки
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Total value of the goods without taxes, without transport and marketing costs and without other additional costs AFTER correction
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Sku">
        <xs:sequence>
            <xs:element name="SourceName" type="tns:SkuSourceName">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Название SKU в учетной системе
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        SKU name as in accounting system
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="EnglishName" type="tns:SkuLocalName">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Название SKU на английском языке
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        SKU name in English
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AdditionalNames" type="tns:SkuNames" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Дополнительные названия SKU
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Additional SKU names
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SkuIdentifier" type="tns:SkuIdentifier" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор SKU согласно классификации партнера (отправителя или получателя сообщения).
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        SKU identifier according to the classification of the partner (the sender or recipient of the message).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SkuMarketingCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Маркетинговый код SKU - код SKU согласно классификации КУБ
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        SKU marketing code, according to CUB classification
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="10"/>
                        <xs:maxLength value="10"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="SkuStatus" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Статус SKU. Зарезервировано.
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        SKU status. Reserved.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SkuBaseUnitOfMeasure">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Базовая единица измерения SKU (может быть только килограмм или штука).
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        SKU base unit of measure (only KG or PCS allowed)
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="KG"/>
                        <xs:enumeration value="PCS"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="TradeItems" type="tns:TradeItems">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Список упаковок SKU. На любом этапе выгрузки должна присутствовать базовая упаковка, при выгрузке SKU готовых к отгрузке также должны присутствовать упаковки “Блок” и “Ящик”.
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        List of SKU packaging. At any time during exchange there should be a base package. For SKU which is ready for shipment, UNIT and BOX are required.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ManufacturerIdentifier" type="tns:InstanceIdentifier" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Код фабрики-производителя согласно классификации CUB
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        factory-manufacturer code according to the CUB classification
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="NationalId" type="tns:SkuNationalId" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Код по национальной классификации (ТНВЭД)
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        National identifier
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InternationalId" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Код товара по международному классификатору
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        International identifier
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IDGPC" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Код продукта соответствует Global Product Classification, см. http://www.gs1.org/gsmp/kc/gpc/latest
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Product code corresponds to the Global Product Classification, see. http://www.gs1.org/gsmp/kc/gpc/latest
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ExpirationTime" type="tns:Expiration" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Срок годности (в днях)
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Expiration time (days)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Notes" type="tns:Notes" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Комментарий
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Comment
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SkuNames">
        <xs:sequence>
            <xs:element name="LocalName" type="tns:SkuLocalizedName" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Название на выбранном языке
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Name in selected language
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SkuIdentifier">
        <xs:sequence>
            <xs:element name="Id" type="tns:InstanceIdentifier">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Идентификатор
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Identifier
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="NodeId" type="tns:InstanceIdentifier">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Код партнера согласно классификации CUB.
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Partner code according to cube classification
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SkuList">
        <xs:sequence>
            <xs:element name="Sku" type="tns:Sku" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Sku
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Sku
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="scenario" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="SkuLocalName">
        <xs:sequence>
            <xs:element name="Name" type="tns:Notes">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Название
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Name
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FullName" type="tns:Notes">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Полное название
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Full name
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ShortName" type="tns:Notes">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Краткое название
                    </xs:documentation>
                    <xs:documentation xml:lang="en">
                        Short name
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SkuSourceName">
        <xs:complexContent>
            <xs:extension base="tns:SkuLocalName">
                <xs:sequence>
                    <xs:element name="LanguageCode" type="tns:LanguageCode">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">
                                Код языка согласно ISO 639-1
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                The language code according to ISO 639-1
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="NodeId" type="tns:InstanceIdentifier">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">
                                Код узла, в котором используется наименование, согласно классификации CUB
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                Node ID, which uses the name according of classification СUB
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="SkuLocalizedName">
        <xs:complexContent>
            <xs:extension base="tns:SkuLocalName">
                <xs:sequence>
                    <xs:element name="CountryCode" type="tns:CountryCode">
